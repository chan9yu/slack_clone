{
	"name": "slack_clone_client",
	"private": true,
	"version": "0.0.0",
	"description": "slack clone client repository",
	"author": "chan9yu <wry5533@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/chan9yu/slack_clone_client/"
	},
	"homepage": "https://chan9yu.github.io/test/",
	"type": "module",
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"deploy": "gh-pages -d dist",
		"commit": "pnpm git-cz",
		"prepare": "husky install",
		"prepare:permission": "chmod +x .husky/*"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@tanstack/react-query": "^4.33.0",
		"axios": "^1.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.15.0",
		"recoil": "^0.7.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@emotion/babel-plugin": "^11.11.0",
		"@tanstack/react-query-devtools": "^4.33.0",
		"@types/node": "^20.5.7",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"commitizen": "^4.3.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"gh-pages": "^6.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"prettier": "^3.0.2",
		"typescript": "^5.0.2",
		"vite": "^4.4.5"
	}
}
